{
    "openapi": "3.0.0",
    "info": {
        "title": "X Money Controller API",
        "version": "1.0.0"
    },
    "paths": {
        "/api/get-access-token/{googleIdToken}": {
            "get": {
                "summary": "Get app token by using google id token",
                "operationId": "0078d7ced2d5502f73d4c972a6700310",
                "parameters": [
                    {
                        "name": "googleIdToken",
                        "in": "path",
                        "description": "use google id token from firebase authentication.",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "returns a sanctum token if successful."
                    }
                }
            }
        },
        "/api/categories": {
            "post": {
                "summary": "Add or update category",
                "operationId": "e0210673ee95266563e9a973956194a1",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "unique_id": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "unique_id": "a3fb6",
                                    "name": "Food"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/api/transactions": {
            "post": {
                "summary": "Add or update transaction",
                "operationId": "9be3876053fa319aa65a3c4eea85afd3",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "unique_id": {
                                        "type": "string"
                                    },
                                    "amount": {
                                        "type": "integer"
                                    },
                                    "note": {
                                        "type": "string"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "unique_id": "a3fb6",
                                    "amount": 100,
                                    "note": "hello"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    }
}